body {background-image:url(images/fundos/fundo.png); background-repeat:repeat-x; 
background-color:#AACCCA; margin:0 0 0 0; height:100%; font-family: Trebuchet MS, Tahoma, Verdana; 
font-size:13px; color:#333333;}

html {margin:0; height:100%}

.sem_estilo {padding:22px;}

a{color:#333333; cursor:pointer; border:none;}
a:hover{text-decoration:none; color:#CC6600}

p {padding:0; margin:0}

ul { list-style:none; margin:0; padding:0;}

input[type=text] {border:solid 1px #74d3ed;}
input[type=text]:hover {background-color:#74d3ed}
textarea {border:solid 1px #74d3ed;}
textarea:hover {background-color:#74d3ed}

#geral {width:775px; height:100%; left:50%; margin-left:-387px; position:absolute; z-index:1}

/*coisas do cabeçalho*/
#topo {width:100%; background-color:#E7FFAE; background-repeat:no-repeat; border-bottom:3px #1E3D49 solid;
position:relative; z-index:100; display:table; font-family:Tahoma, Verdana}
#centraliza_topo { width:775px; margin:auto}
#hist {float:left; font-size:14px; margin:4px}
#bt_ajuda {cursor:pointer; float:right; font-size:12px; 
font-weight:bold; margin:6px; color:#333333}
#bt_ajuda:hover {color:#CC6600}

#cabecalho {width:100%; position:relative; height:150px}
#ajuda {width:100%; position:relative; display:block; z-index:1}
#ajuda_meio {background-image:url(images/fundos/tira.png); background-repeat:repeat-y;  width:100%; height:90px}
#ajuda_base {background-image:url(images/fundos/corpo_base2.png); background-repeat:no-repeat; 
width:100%; height:59px}
#ajudante {width:680px; height:100%; position:absolute; margin-left:47px; margin-right:47px}
#personagem {float:left; height:100%; width:20%}
#balao {float:right; width:74%; background-color:white; padding:20px; -moz-border-radius:30px;
-webkit-border-radius:30px; border-radius:30px}
#rel {position:relative; z-index:inherit; zoom:1; height:100%} /*para funcionar borda arredondada nos IE*/

/*coisas do conteudo*/
#conteudo_topo {background-image:url(images/fundos/corpo_topo.png); background-repeat: no-repeat; 
width:100%; height:59px; position:relative; margin-top:20px;}
#conteudo_meio {background-image:url(images/fundos/tira.png); background-repeat:repeat-y; 
width:100%; position:relative; z-index:100}
#conteudo_base {background-image:url(images/fundos/corpo_base.png); background-repeat: no-repeat; 
width:100%; height:59px; position:relative}

#conteudo {width:680px; margin-left:47px; margin-right:47px; margin-top:-10px; position:absolute}

.bts_cima {width:100%; float:left; margin-bottom:22px}
.bts_baixo {width:100%; float:left; margin-top:22px}

.bloco {background-color:#EEF5F5; border-top:25px solid #53686F; border-left:1px solid #aaaaaa;
border-right:1px solid #aaaaaa; border-bottom:1px solid #aaaaaa; min-height:20px}
.move_bloco {border:white dotted 2px; height:50px; margin-bottom:22px; width:234px;}

h1 {font-size:15px; margin-top:-22px; color:white; font-family:Tahoma, Verdana; text-indent:22px; 
position:relative; letter-spacing:1px; font-weight:normal; cursor:default; margin-bottom:4px}

.tabela {display:table; width:100%}

.cor1 { display:table; width:100%; background-color:#EEF5F5}

.cor2 { display:table; width:100%; background-color:#ccecf4}

.dados { font-weight:bold; display:table-cell; padding-right:7px}
.valor { display:table-cell; width:100%}

.tabela_oculta { width:100%; margin-bottom:15px;}

.toggle { color:#FFFFFF}
.toggle:hover { color:#E7FFAE}

#comentarios {width:428px; height:80%; display:none; position:absolute; z-index:1000; background-color:#F4FFDD; top:50%; left:50%}
#comentarios textarea {margin-bottom:22px}

.msg_dimensao {width:100%}

#novo_coment {margin-top:15px; border-top:1px solid; padding-top:5px; margin-bottom:15px}

#light_box {width:100%; height:100%; background-color:#ffffff; -moz-opacity:0.5; opacity:0.5; 
filter: alpha(opacity=50); display:none; z-index:500; position:absolute;}

.recebe_coments {overflow:auto; height:100%; position:relative}

.fechar_coments {float:right; display:table-cell; margin-top:-25px; position:relative; cursor:pointer}

.troca_paginas {background-color:#EEF5F5; width:100%; float:left}
.troca_paginas a {margin-left:3px; margin-right:3px; text-decoration:none; color:#000000; 
font-family:Trebuchet MS, Tahoma, Verdana, Sans; font-size:10pt;}
.troca_paginas a:hover {color:#C60;}

.paginas_padding { padding:10px}

.numero {background-color:#74D3ED; padding:2px; padding-left:8px; padding-right:8px;}
.numero:hover {background-color:#C5E9EF}

.numero_atual {background-color:#C5E9EF; padding:2px; padding-left:8px; padding-right:8px;}

.espaco {margin-top:10px;}

.primeira {padding:4px;}

.seguinte {color:#9ED2DB; padding:4px;}

.margem_paginas { margin-bottom:12px; margin-top:12px}